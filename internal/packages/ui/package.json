{
  "name": "@vercel/examples-ui",
  "version": "2.0.3",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build:swc -w",
    "build": "rm -rf dist && pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "types": "tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf ./dist && rm -rf .turbo"
  },
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./tailwind": {
      "require": "./dist/tailwind.cjs"
    },
    "./globals.css": "./dist/globals.css"
  },
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts,css}"
  ],
  "typesVersions": {
    "*": {
      "index": [
        "src/index"
      ],
      "tailwind": [
        "dist/tailwind.d.cts"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "index": [
          "dist/index.d.ts"
        ],
        "tailwind": [
          "dist/tailwind.d.cts"
        ]
      }
    }
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.2 || ^18.0.0-0",
    "react-dom": "^17.0.2 || ^18.0.0-0"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.6",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.1",
    "@types/react": "^18.2.55",
    "chokidar": "^3.6.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.5.6",
    "next": "^13.4.10",
    "typescript": "^5.3.3"
  }
}
